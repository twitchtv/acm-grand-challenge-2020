  "dependencies": {
    "puppeteer": "^2.1.1",
    "puppeteer-core": "^2.1.1"
  },
  "devDependencies": {},
  "config": {
    "ffmpeg_profile": "PROFILE_NORMAL",
    "network_profile": "PROFILE_CASCADE"
  },
  "scripts": {
